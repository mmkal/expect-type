    "arethetypeswrong": "attw --pack",
    "test": "vitest run"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@types/node": "^22.0.0",
    "@typescript/native-preview": "7.0.0-dev.20251021.1",
    "@vitest/ui": "^3.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-mmkal": "0.9.0",
    "np": "^10.2.0",
    "pkg-pr-new": "0.0.60",
    "strip-ansi": "7.1.2",
    "ts-morph": "27.0.2",
    "typescript": "5.9.3",
    "vitest": "^3.0.0"
  }
}
